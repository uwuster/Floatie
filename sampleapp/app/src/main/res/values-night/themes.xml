<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Sampleapp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/indigo_200</item>
        <item name="colorPrimaryVariant">@color/indigo_200</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/indigo_100</item>
        <item name="colorSecondaryVariant">@color/indigo_100_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorFloatieButtonText">@color/indigo_100</item>
        <item name="colorFloatieButtonBackground">@color/black</item>
        <item name="colorFloatieButtonStroke">@color/indigo_200_dark</item>
        <item name="colorFloatieButtonRipple">@color/indigo_200_dark</item>
    </style>
</resources>